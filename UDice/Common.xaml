<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


	<Viewbox x:Key="CrossGlyph">
		<Path Data="M0,0 L1,1 M0,1 L1,0" Margin="0.3"  StrokeThickness="0.2"/>
	</Viewbox>
	<Viewbox x:Key="MinimizeGlyph">
		<Path Data="M0,0.8 L1,0.8" Margin="0.3"  StrokeThickness="0.2" />
	</Viewbox>
</ResourceDictionary>